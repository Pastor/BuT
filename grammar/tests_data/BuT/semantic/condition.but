
model Base {
    state One {
        ref End: Writable & HasNext;
    }
    cond Readable = is_readable & (register_5 | reg[0]);
    cond Writable = !Readable & is_writable;
    cond HasNext  = reg[5] & point > 50; //Хотелось бы использовать как reg.5, но сейчас есть проблема с детектированием числа с плавающей точкой
}
