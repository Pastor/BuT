#![align(4)] // align 4 byte, default 1
/// Standard
type u8    = [8   : bit];
type u16   = [16  : bit];
type u32   = [32  : bit];
type u64   = [64  : bit];
type u128  = [128 : bit];
type bool  = bit;
type int   = u32; // u16
type long  = u32;
/// Custom
type u5    = [5 : bit];
type block = [2 : int];
type block2= [2 : [2: int]]; //Nested type

